# Development environment configuration
ENVIRONMENT=development
GCP_PROJECT_ID=healthcare-ai-dev
GCP_LOCATION=us-central1
DIALOGFLOW_AGENT_ID=your-agent-id-here
GEMINI_MODEL=gemini-pro
PUBSUB_TOPIC=conversation-events

# Genesys Cloud
GENESYS_CLIENT_ID=your-client-id
GENESYS_CLIENT_SECRET=your-client-secret
GENESYS_ENVIRONMENT=mypurecloud.com
GENESYS_WEBHOOK_SECRET=your-webhook-secret

# CRM Configuration
CRM_PROVIDER=salesforce
CRM_API_ENDPOINT=https://your-instance.salesforce.com
CRM_API_KEY=your-api-key

# Security
ENABLE_PHI_REDACTION=true
ENABLE_AUDIT_LOGGING=true
JWT_SECRET=your-jwt-secret-here
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Service Settings
AGENT_ASSIST_ENABLED=true
LLM_CONFIDENCE_THRESHOLD=0.7
MAX_CONVERSATION_HISTORY=10

# Performance
REQUEST_TIMEOUT=30
MAX_RETRIES=3
ENABLE_CACHING=true

# Flask
FLASK_ENV=development
FLASK_DEBUG=true
